/* pages/outwarehouseapplication/outwarehouseapplication.wxss */
/* 提示信息垂直居中 */
.toast-center{
  margin-top: 160px;
}
/* 导航栏置顶 */
.navbar-fixed{
  position: fixed;
  top: 0px;
  background-color: #f8f8f8;
}

 /* 表格标题 */
.tabel-title{
  position: fixed;
  display: flex;
  justify-content: flex-start;
  padding-left:35rpx;
  padding-right:35rpx;
  font-size:14px;
  z-index: 50;
  top: 50px;
  background-color: #f8f8f8;
}
.tabel-title-text{
  padding-left: 12rpx;
  padding-right: 12rpx;
  min-width: 460rpx;
  color: #999;
}

.delete-icon{ /* 删除按钮 */
  position: relative;
  top: -1px;
  left: 10px;
  z-index: 200;
}
.required-icon{  /* 必填标志 */
  padding: 0px;
  font-size: 20px;
  background-color: white;
  color: #e64340;
}

.text-area{
  padding: 10px 15px;
}
.label-wrap{
  height: 100%;
  min-height: 100%;
  padding-top: 10px;
  padding-bottom: 10px;
  line-height: 1.41176471;
  word-wrap: break-word;
  white-space: normal;
}

/* 提交按钮 */
.submit-button{
  width: 50%;
}